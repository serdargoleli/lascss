{
  "name": "@las/webpack",
  "version": "1.0.0",
  "description": "LASCSS JIT Webpack plugin",
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "clean": "rimraf dist",
    "build": "tsup src/index.ts --format cjs,esm --dts --clean",
    "dev": "tsup src/index.ts --format cjs,esm --dts --watch"
  },
  "dependencies": {
    "@las/lasgine": "workspace:*",
    "webpack-virtual-modules": "^0.6.2"
  },
  "peerDependencies": {
    "webpack": "^5.0.0"
  },
  "devDependencies": {
    "@types/html-webpack-plugin": "^3.2.9",
    "@types/node": "^20.0.0",
    "@types/webpack": "^5.28.5",
    "html-webpack-plugin": "^5.6.5",
    "rimraf": "^6.1.2",
    "tsup": "^8.5.1",
    "typescript": "^5.7.2"
  },
  "sideEffects": false
}