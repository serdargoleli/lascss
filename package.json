{
  "name": "lascss-repo",
  "private": true,
  "description": "LAS CSS Monorepo",
  "engines": {
    "node": ">=18",
    "pnpm": ">=9"
  },
  "packageManager": "pnpm@10.24.0",
  "scripts": {
    "dev": "turbo run dev --parallel",
    "dev:webpack": "turbo run dev --filter=@las/webpack --filter=las-webpack-example --parallel",
    "build:webpack": "turbo run build --filter=@las/webpack --filter=las-webpack-example --parallel",
    "build": "turbo run build",
    "clean": "turbo run clean",
    "clean-all": "pnpm -r exec rm -rf node_modules .turbo dist",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,md}\"",
    "release": "lerna publish",
    "version": "lerna version"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^8.48.0",
    "@typescript-eslint/parser": "^8.48.0",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "lerna": "^9.0.3",
    "prettier": "^3.7.1",
    "rimraf": "^6.1.2",
    "ts-node": "^10.9.2",
    "turbo": "^2.6.1",
    "typescript": "^5.7.2"
  }
}